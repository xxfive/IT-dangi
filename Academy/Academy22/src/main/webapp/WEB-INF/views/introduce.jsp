<script>
(function(){
	window.ad_app = {};

	ad_app.submit = function(){
		var user_name = $("input[name=user_name]").val();
		var user_email = $("input[name=user_email]").val();
		var user_number = $("input[name=user_number]").val();
		var user_site = $("select[name=user_site]").val();
		var user_title = $("input[name=user_title]").val();
		var user_cont = $("textarea[name=user_cont]").val();
		var agree = $("input[name=agree]").is(":checked");
		// var recaptcha = $("textarea[name='g-recaptcha-response']").val();

		if(!user_name){
			alert("이름을 입력해주세요.");
			$("input[name=user_name]").focus();
			return;
		}

		if(!user_email){
			alert("이메일주소를 입력해주세요.");
			$("input[name=user_email]").focus();
			return;
		}

		var emailRegExp = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		if(user_email.match(emailRegExp) == null){
			alert("올바른 이메일주소를 입력해주세요.");
			$("input[name=user_email]").focus();
			return;
		}

		if(!user_number){
			alert("전화번호를 입력해주세요.");
			$("input[name=user_number]").focus();
			return;
		}

		var phoneRegExp = /^(01[016789]{1}|02|0[3-9]{1}[0-9]{1})-?[0-9]{3,4}-?[0-9]{4}$/;

		if(user_number.match(phoneRegExp) == null){
			alert("올바른 전화번호를 입력해주세요.");
			$("input[name=user_number]").focus();
			return;
		}

		if(!user_site){
			alert("서비스를 선택해주세요.");
			$("select[name=user_site]").focus();
			return;
		}

		if(!user_title){
			alert("제목을 입력해주세요.");
			$("input[name=user_title]").focus();
			return;
		}

		if(!user_cont){
			alert("내용을 입력해주세요.");
			$("textarea[name=user_cont]").focus();
			return;
		}

		if(!agree){
			alert("개인정보수집에 동의해주세요.");
			$("input[name=agree]").focus();
			return;
		}

		// if(!recaptcha){
		// 	alert("reCAPTCHA를 체크해주세요.");
		// 	return;
		// }

		$('form[name=email_board]').submit();
	}
})();
</script>
